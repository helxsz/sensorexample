       <ul class="nav pull-right row">
	    <li>
	        <a href="/profile/<%= locals.user.username %>" class="b-userbar__icons-item"> 
                <img src="http://commondatastorage.googleapis.com/johnius/anchor.png" width="25" height="25" alt="Userbar icon"> 
                <span class="b-userbar__icons-item-notify">5</span> 
            </a>
		</li>	
	    <li>
	     <a href="#" class="b-userbar__account ">
		            <% if(locals.user.img) { %>
                        <img src="/images/<%= locals.user.img %>" width="21" height="21" alt="User avatar" class="b-userbar__account-avatar">
					<% } %>
					<% if(!locals.user.img) { %>
                        <img src="/default_user.jpeg" width="21" height="21" alt="User avatar" class="b-userbar__account-avatar">					
					<% } %>
         </a>
		</li> 
        <li class="dropdown ">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" ><%= locals.user.username %></a>
          <ul class="dropdown-menu">
		    <li><a href="/profile/<%= locals.user.username %>">Dashboard</a></li>
            <li><a href="/settings/profile">Edit Profile</a></li>
            <li><a href="/settings/account">Account</a></li>
            <li class="divider"></li>			
		    <li>  
		      <form method="POST" class="form-inline" action="/sessions" enctype="application/x-www-form-urlencoded">
	             <input type="hidden" name="_csrf" value="<%= token %>"/>
			     <input type="hidden" name="_method" value="delete" />			 
			     <button id="btn-logout" class="btn btn-primary"><i class="icon-lock icon-white"></i>Sign Out</button>			 
              </form>
		    </li>						
          </ul>
        </li>
       </ul>	 
	  <% } %>

    </div>
  </div> 

